{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "5595626d-df14-4efb-abb5-14c8730ce177"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "RJ_NOC_Application": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rishi",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets/appsettings.json",
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/custom-theme.scss",
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/PortalCss.css",
              "src/assets/css/font-awesome/css/font-awesome.min.css",
              "src/assets/css/bootstrap-icons.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/fancybox.css",
              "./node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.6.4.min.js",
              //"src/assets/js/owl.carousel.min.js",
              //"src/assets/js/highcharts.js",
              "src/assets/js/zingchart.min.js",
              "src/assets/js/fancybox.umd.js",
              "src/assets/js/custom.js",
              "src/assets/js/bootstrap.bundle.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "RJ_NOC_Application:build:production"
            },
            "development": {
              "browserTarget": "RJ_NOC_Application:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RJ_NOC_Application:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
